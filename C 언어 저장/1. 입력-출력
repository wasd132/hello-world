#include<stdio.h>
void main()
{
	int a;
	int b;
	int c;
	int d;
	printf("1월 수익금:");
	scanf_s("%d", &a);
	printf("2월 수익금:");
	scanf_s("%d", &b);
	printf("3월 수익금:");
	scanf_s("%d", &c);
	printf("4월 수익금:");
	scanf_s("%d", &d);
    
	int e = a + b + c + d;
	float avg=e/4;
	printf("1분기 수익금 평균: %f",avg);
		
}
