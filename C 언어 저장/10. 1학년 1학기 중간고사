#include<stdio.h>
void main()
{
	int a=0, b=0;
	printf("정수입력:");
	scanf_s("%d%d", &a, &b);
	if (a > b)
	{
		printf("첫번째 값인%d가 더 크다.", a);
	}
	else if (a < b)
	{
		printf("두번째값인%d가 더 크다", b);
	}
	else if (a == b)
	{
		printf("%d,%d 둘다 같다.", a, b);
	}
}

--------------------------------------
#include<stdio.h>
void main()
{
	int a, b;
	printf("정수입력:");
	scanf_s("%d", &a);
	printf("정수입력:");
	scanf_s("%d", &b);
	if (a % b == 0)
	{
		printf("나누어 떨어진다.");
	}
	else { printf("나누어 떨어지지 않는다."); }
}
